<%- include("../partials/header.ejs")%>

<h2 class="text-warning mt-5 ml-5">
	ADD DETAILS OF NEW SONG :
</h2>


<div class="row justify-content-center">
	<div class="col-lg-5 col-md-7 col-sm-10">

		<form class="p-5 " action="/musics" method="POST">
			<div class="form-group">
				<input type="text" class="form-control " placeholder="title of song" name="title">
			</div>
			<div class="form-group">
				<input type="text" class="form-control " placeholder="artist" name="artist">
			</div>
			<div class="form-group">
				<input type="text" class="form-control " placeholder="image url" name="image">
			</div>
			<div class="form-group">
				<textarea rows="3" placeholder="decription" class="form-control " name="description"></textarea>
			</div>
			<button class="btn btn-block btn-success">
				Submit
			</button>
			
		</form>
	</div>

</div>




<%- include("../partials/footer.ejs")%>